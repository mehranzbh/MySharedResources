<script>
    var num1 = 30
    var num2 = 20
    var strvar1 = "Mehran"
    var strvar2 = "another"

    if (num1 > num2){
        alert('num1>num2')
        } 
        else if (num1 === num2){
            alert ('numbers are the same')
        }
        else if (strvar1 == strvar2){
            alert ('strvar1 = strvar2')
        }
        else {
            alert ('Else is activated')
    }

</script>

	
